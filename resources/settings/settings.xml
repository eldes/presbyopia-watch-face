<settings>
  <setting propertyKey="@Properties.AppVersion" title="@Strings.AppVersionTitle">
    <settingConfig type="alphaNumeric" readonly="true" />
  </setting>

  <setting propertyKey="@Properties.ColorScheme" title="@Strings.ColorSchemeTitle">
    <settingConfig type="list">
      <listEntry value="0x000">@Strings.ColorSchemeWhiteOnBlack</listEntry>
      <listEntry value="0x001">@Strings.ColorSchemeYellowOnBlack</listEntry>
      <listEntry value="0x002">@Strings.ColorSchemeGreenOnBlack</listEntry>
      <listEntry value="0x003">@Strings.ColorSchemeCyanOnBlack</listEntry>
      <listEntry value="0x004">@Strings.ColorSchemeMagentaOnBlack</listEntry>
      <listEntry value="11">@Strings.ColorSchemeYellowOnBlue</listEntry>
      <listEntry value="100">@Strings.ColorSchemeBlackOnWhite</listEntry>
      <listEntry value="110">@Strings.ColorSchemeBlackOnYellow</listEntry>
      <listEntry value="111">@Strings.ColorSchemeBlueOnYellow</listEntry>
    </settingConfig>
  </setting>

  <setting propertyKey="@Properties.UseLeadingZero" title="@Strings.PropertiesUseLeadingZeroTitle">
    <settingConfig type="boolean" />
  </setting>

</settings>
